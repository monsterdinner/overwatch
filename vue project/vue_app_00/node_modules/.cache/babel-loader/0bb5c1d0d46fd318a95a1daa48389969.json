      return {\r\n                list:[],\r\n                pno:1,\r\n                pagesize:8,\r\n            }\r\n        },\r\n        methods:{\r\n            getNewsList(){\r\n                var url=\"Newslist/list\"+\"?pno=\"+this.pno+\"&pageSize=\"+this.pagesize;\r\n                this.$http.get(url).then(result => {\r\n                    result.body.code == 1\r\n                    ? this.list = result.body.msg.data\r\n                    : Toast(\"加载新闻列表失败...\");\r\n                });\r\n            },\r\n            ten(a){\r\n                a.click++;\r\n            },\r\n            loadMore(){\r\n                this.pno++;\r\n                var url=\"Newslist/list\"+\"?pno=\"+this.pno+\"&pageSize=\"+this.pagesize;\r\n                this.$http.get(url).then(result => {\r\n                    result.body.code == 1\r\n                    ? (this.list = this.list.concat(result.body.msg.data))\r\n                    : Toast(\"加载新闻列表失败...\");\r\n                });\r\n                console.log(this.list)\r\n            }\r\n        },\r\n        created() {\r\n            this.getNewsList();\r\n        },\r\n    }\r\n</script>\r\n<style>\r\n    .app-newslist .mui-table-view h1{\r\n        font-size: 14px;\r\n        \r\n    }\r\n    .app-newslist li .mui-ellipsis{\r\n        font-size: 12px;\r\n        color:#226aff;\r\n        display:flex;\r\n        justify-content: space-between;\r\n    }\r\n</style>"],"sourceRoot":"src\\components\\news"}]}