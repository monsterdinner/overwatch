op\r\n           //console.log(scrollTop)\r\n           var offsetTop = document.querySelector('#navbar1').offsetTop\r\n           if(scrollTop>44){\r\n                this.Snavbar={top:\"0\"}\r\n                this.Snavbarmain={marginTop:\"0px\", marginLeft: \"0px\", marginRight: \"0px\"}\r\n           }else{\r\n           this.Snavbar={top:\"44px\"}\r\n                           this.Snavbarmain={marginTop:\"20px\", marginLeft: \"20px\", marginRight: \"20px\"}\r\n           }\r\n         },\r\n              shownav(){\r\n             if(this.fal==false){\r\n             this.fal=true;\r\n             this.fal1=false;\r\n             this.fal2=false;\r\n             this.fal3=true;\r\n             }else if(this.fal==true){\r\n             this.fal=false;\r\n             this.fal3=false;\r\n             }\r\n             },\r\n         shownav1(){\r\n             if(this.fal1==false){\r\n             this.fal1=true;\r\n             this.fal=false;\r\n             this.fal2=false;\r\n             this.fal3=true;\r\n             }else if(this.fal1==true){\r\n             this.fal1=false;\r\n             this.fal3=false;\r\n             }\r\n             },\r\n             shownav2(){\r\n               if(this.fal2==false){\r\n               this.fal2=true;\r\n               this.fal=false;\r\n               this.fal1=false;\r\n               this.fal3=true;\r\n               }else if(this.fal2==true){\r\n              this.fal2=false;\r\n               this.fal3=false;\r\n              }\r\n               },\r\n               black(){\r\n                 if(this.fal3==true){\r\n                  this.fal3=false;\r\n                  this.fal1=false;\r\n                  this.fal2=false;\r\n                  this.fal=false;\r\n               }\r\n               },\r\n                 overopen(){\r\n             this.isopen=true;\r\n        },\r\n        overclose(){\r\n           this.isopen=false;\r\n        }\r\n        },\r\n        created(){\r\n\r\n        },\r\n     watch: {//使用watch 监听$router的变化\r\n         $route(to, from) {\r\n      //如果to索引大于from索引,判断为前进状态,反之则为后退状态\r\n      if(to.meta.index > from.meta.index){\r\n        //设置动画名称\r\n        this.transitionName = 'slide-left';\r\n      }else{\r\n           this.transitionName = 'slide-right';\r\n        }\r\n         }\r\n        },\r\n     mounted () {\r\n            window.addEventListener('scroll', this.handleScroll)\r\n          },\r\n        components:{\r\n               \"app-header\":header\r\n        },\r\n        destroyed () {\r\n          window.removeEventListener('scroll', this.handleScroll)\r\n           \r\n        } \r\n}\r\n</script>"],"sourceRoot":"src\\components"}]}