rollParam.way === 0) {\n        return;\n      }\n\n      var sp = self.scrollParam;\n      self.evUtils.preventDefault(e);\n      self.elUtils.constantSpeedToTop(self.elUtils.getDomScrollTop(), sp.time, sp.sportWay, sp.slowWay, sp.callback);\n    },\n    // 计算滚动条位置\n    computeScroll: function computeScroll() {\n      var self = this;\n      self.evUtils.addHandler(window, 'scroll', function () {\n        self.showGoTop = self.elUtils.getDocScrollTop() >= self.scrollParam.distance;\n      });\n    }\n  }\n};",{"version":3,"sources":["scrollTop.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;AAkBA,eAAA;AACA,SAAA;AACA;;;AAGA,iBAAA;AACA,YAAA,MADA;AAEA,aAFA,sBAEA;AACA,eAAA;AACA;AACA,eAAA,CAFA;AAGA;AACA,oBAAA,EAJA;AAKA;AACA,gBAAA,GANA;AAOA;AACA,oBAAA,MARA;AASA;AACA,mBAAA,QAVA;AAWA;AACA,oBAAA;AAZA,SAAA;AAcA;AAjBA;AAJA,GADA;AAyBA,MAzBA,kBAyBA;AACA,WAAA;AACA;AACA,eAAA,KAAA,KAAA,CAAA,OAFA;AAGA;AACA,eAAA,KAAA,KAAA,CAAA,KAJA;AAKA;AACA,iBAAA;AANA,KAAA;AAQA,GAlCA;AAmCA,YAAA,EAnCA;AAqCA,SArCA,qBAqCA;AACA,SAAA,aAAA;AACA,GAvCA;AAwCA,WAAA;AACA;AACA,SAFA,iBAEA,CAFA,EAEA;AACA,UAAA,OAAA,IAAA;AACA,UAAA,KAAA,OAAA,CAAA,QAAA,CAAA,CAAA,CAAA;;AACA,UAAA,KAAA,WAAA,CAAA,GAAA,KAAA,CAAA,EAAA;AACA;AACA;;AACA,UAAA,KAAA,KAAA,WAAA;AACA,WAAA,OAAA,CAAA,cAAA,CAAA,CAAA;AACA,WAAA,OAAA,CAAA,kBAAA,CAAA,KAAA,OAAA,CAAA,eAAA,EAAA,EAAA,GAAA,IAAA,EAAA,GAAA,QAAA,EAAA,GAAA,OAAA,EAAA,GAAA,QAAA;AACA,KAXA;AAYA;AACA,iBAbA,2BAaA;AACA,UAAA,OAAA,IAAA;AACA,WAAA,OAAA,CAAA,UAAA,CAAA,MAAA,EAAA,QAAA,EAAA,YAAA;AACA,aAAA,SAAA,GAAA,KAAA,OAAA,CAAA,eAAA,MAAA,KAAA,WAAA,CAAA,QAAA;AACA,OAFA;AAGA;AAlBA;AAxCA,CAAA","sourcesContent":["<!--\r\n    path: src/components/practical/scrollTop.vue\r\n    author: wujiang\r\n    time: 2018/4/12\r\n    desc: 回到顶部组件\r\n-->\r\n<template>\r\n    <section>\r\n        <section :class=\"['utils-sticky-btns', {'scrolled': showGoTop}]\">\r\n            <a href=\"#\" @click=\"goTop($event)\">\r\n                <span>\r\n                    <i class=\"fa fa-chevron-up fa-2x\" aria-hidden=\"true\"></i>\r\n                </span>\r\n            </a>\r\n        </section>\r\n    </section>\r\n</template>\r\n<script>\r\nexport default {\r\n    props: {\r\n        /*\r\n         * 返回顶部可用参数\r\n         */\r\n        scrollParam: {\r\n            type: Object,\r\n            default () {\r\n                return {\r\n                    // 回到顶部的方式 0 - 马上回到顶部，css实现（默认） 1 - 匀速回到顶部，js实现\r\n                    way: 0,\r\n                    // 滚动多少像素显示“回到顶部”图标\r\n                    distance: 20,\r\n                    // 向上滚动间隔\r\n                    time: 600,\r\n                    // 运动方式\r\n                    sportWay: 'Quad',\r\n                    // 缓急方式\r\n                    slowWay: 'easeIn',\r\n                    // 回到顶部后回调方法\r\n                    callback: undefined\r\n                }\r\n            }\r\n        }\r\n    },\r\n    data () {\r\n        return {\r\n            // 操作element的方法\r\n            elUtils: this.$util.element,\r\n            // 操作event的方法\r\n            evUtils: this.$util.event,\r\n            // 是否展示“回到顶部”图标\r\n            showGoTop: false\r\n        }\r\n    },\r\n    computed: {\r\n    },\r\n    mounted () {\r\n        this.computeScroll()\r\n    },\r\n    methods: {\r\n        // 回到顶部\r\n        goTop (e) {\r\n            let self = this\r\n            e = self.evUtils.getEvent(e)\r\n            if (self.scrollParam.way === 0) {\r\n                return\r\n            }\r\n            let sp = self.scrollParam\r\n            self.evUtils.preventDefault(e)\r\n            self.elUtils.constantSpeedToTop(self.elUtils.getDomScrollTop(), sp.time, sp.sportWay, sp.slowWay, sp.callback)\r\n        },\r\n        // 计算滚动条位置\r\n        computeScroll () {\r\n            let self = this\r\n            self.evUtils.addHandler(window, 'scroll', function () {\r\n                self.showGoTop = self.elUtils.getDocScrollTop() >= self.scrollParam.distance\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n/*回到顶部标识*/\r\n.utils-sticky-btns.scrolled{\r\n    right: 3px;\r\n}\r\n.utils-sticky-btns{\r\n    position: fixed;\r\n    z-index: 50;\r\n    right: -48px;\r\n    bottom: 10px;\r\n    -webkit-transition: right .3s ease;\r\n    transition: right .3s ease;\r\n}\r\n\r\n.utils-sticky-btns a{\r\n    text-decoration: underline;\r\n    color: #ff712c;\r\n    -webkit-transition: all .3s ease;\r\n    transition: all .3s ease;\r\n}\r\n\r\n.utils-sticky-btns span{\r\n    display: block;\r\n    float: left;\r\n    width: 42px;\r\n    height: 42px;\r\n    text-align: center;\r\n    font-size: 1.25em;\r\n    color: #fff;\r\n    background: #ff712c;\r\n    border-radius: 4px;\r\n    padding-top: 6px;\r\n    margin: 0 5px;\r\n    cursor: pointer;\r\n    -webkit-transition: background .3s ease;\r\n    transition: background .3s ease;\r\n}\r\n\r\n.utils-sticky-btns span:hover{\r\n    background: #f85100;\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\components"}]}