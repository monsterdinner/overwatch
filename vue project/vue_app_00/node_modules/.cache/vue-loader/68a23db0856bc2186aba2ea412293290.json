s=\"mui-media-body\">\r\n                        <h1>{{item.title}}</h1>\r\n                        <p class='mui-ellipsis'>\r\n                            <span>{{item.ctime|dateFormat}}</span>\r\n                            <span>点击:{{item.click}} 次</span>\r\n                        </p>\r\n                    </div>\r\n                </router-link>\r\n            </li>\r\n        </ul>\r\n        <mt-button @click=\"loadMore()\" type=\"primary\" size=\"large\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n    import {Toast} from 'mint-ui';\r\n    export default{\r\n        name:\"news\",\r\n        data(){\r\n            return {\r\n                list:[],\r\n                pno:1,\r\n                pagesize:8,\r\n            }\r\n        },\r\n        methods:{\r\n            getNewsList(){\r\n                var url=\"Newslist/list\"+\"?pno=\"+this.pno+\"&pageSize=\"+this.pagesize;\r\n                this.$http.get(url).then(result => {\r\n                    result.body.code == 1\r\n                    ? this.list = result.body.msg.data\r\n                    : Toast(\"加载新闻列表失败...\");\r\n                });\r\n            },\r\n            ten(a){\r\n                a.click++;\r\n            },\r\n            loadMore(){\r\n                this.pno++;\r\n                var url=\"Newslist/list\"+\"?pno=\"+this.pno+\"&pageSize=\"+this.pagesize;\r\n                this.$http.get(url).then(result => {\r\n                    result.body.code == 1\r\n                    ? (this.list = this.list.concat(result.body.msg.data))\r\n                    : Toast(\"加载新闻列表失败...\");\r\n                });\r\n                console.log(this.list)\r\n            }\r\n        },\r\n        created() {\r\n            this.getNewsList();\r\n        },\r\n    }\r\n</script>\r\n<style>\r\n    .app-newslist .mui-table-view h1{\r\n        font-size: 14px;\r\n        \r\n    }\r\n    .app-newslist li .mui-ellipsis{\r\n        font-size: 12px;\r\n        color:#226aff;\r\n        display:flex;\r\n        justify-content: space-between;\r\n    }\r\n</style>"]}]}