"file":"HomeContainer.vue","sourceRoot":"src\\components\\tabbar","sourcesContent":["<template>\r\n    <div class=\"app\">\r\n        <!-- 主页组件 -->\r\n        <mt-swipe :auto=\"4000\" style=\"height:200px\">\r\n            <mt-swipe-item v-for=\"item in list\" :key=\"item.id\">\r\n                <img :src=\"item.img_url\" alt=\"\" class=\"imgCover\">\r\n            </mt-swipe-item>\r\n        </mt-swipe>\r\n        <ul class=\"mui-table-view mui-grid-view mui-grid-9\">\r\n\t\t    <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\">\r\n                <router-link to=\"/home/newslist\">\r\n                    <img src=\"../../img/menu1.png\" />\r\n                    <div class=\"mui-media-body\">新闻资讯</div>\r\n                </router-link>\r\n            </li>\r\n\t\t    <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\">\r\n                <router-link to=\"/home/goodlist\">\r\n                    <img src=\"../../img/menu2.png\" />\r\n                    <div class=\"mui-media-body\" id=\"badge\">商品</div>\r\n                </router-link>\r\n            </li>\r\n\t\t    <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\">\r\n                <a href=\"#\">\r\n                    <img src=\"../../img/menu3.png\" />\r\n                    <div class=\"mui-media-body\">购物车</div>\r\n                </a>\r\n            </li>\r\n\t\t    <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\">\r\n                <a href=\"#\">\r\n                    <img src=\"../../img/menu4.png\" />\r\n                    <div class=\"mui-media-body\">支付</div>\r\n                </a>\r\n            </li>\r\n\t\t    <li class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\">\r\n                <a href=\"#\">\r\n                    <img src=\"../../img/menu5.png\" />\r\n                    <div class=\"mui-media-body\">订单</div>\r\n                </a>\r\n            </li>\r\n\t\t    <li class=\"mui-table-view